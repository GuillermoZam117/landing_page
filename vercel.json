{
    "version": 2,
    "builds": [
        {
            "src": "**/*",
            "use": "@vercel/static"
        }
    ],
    "routes": [
        {
            "src": "/",
            "dest": "/landing.html"
        },
        {
            "src": "/resources/(.*)",
            "dest": "/resources/$1"
        },
        {
            "src": "/(.*)",
            "dest": "/$1"
        }
    ]
} 